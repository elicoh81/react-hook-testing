    "@types/jest": "^24.0.24",
    "@types/react": "^16.9.14",
    "@types/react-dom": "^16.9.4",
    "@types/react-redux": "^7.1.5",
    "@types/redux-actions": "^2.6.1",
    "@types/redux-mock-store": "^1.0.1",
    "@types/redux-thunk": "^2.1.0",
    "babel-core": "^7.7.7",
    "babel-jest": "^24.9.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
